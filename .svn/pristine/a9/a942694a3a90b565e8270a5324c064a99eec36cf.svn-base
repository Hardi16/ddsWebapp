@media (min-width: 768px) and (max-width: 1024px) {
  .patientDetails {
    height: 250px;
    max-height: calc(100% - 190px);
  }
  .leftPaneHeaderFix {
    position: relative !important;
    width: 100% !important;
  }
  .leftHeaders {
    justify-content: center;
  }
  .leftPaneSearchCreatePage {
    width: 100%;
  }
}
@media only screen and (max-width: 768px) {
  .mainContainer {
    flex-direction: column;
    overflow-y: auto;
  }
  .leftPaneContent {
    padding: 15px;
  }
  .leftPaneHeaderFix {
    width: 100%;
  }
  .rightPane {
    padding: 15px 0;
    flex-direction: column;
  }
  .tempCard1 {
    margin: 10px 15px 10px 5px;
  }
  .collapse {
    padding-top: 10px;
  }
  .react-datepicker-popper {
    right: 0;
    left: auto;
  }
  #draggable {
    width: 100%;
    overflow: hidden;
  }
  textarea {
    width: 100%;
  }
  .doctorCardApprovalPage {
    width: 100%;
    height: auto;
    margin: 0px;
  }
  .datepicker {
    width: 100%;
  }
  .react-datepicker-popper {
    right: 0;
    left: auto;
  }
}
.leftPaneHeaderFix {
  position: fixed;
  width: 25%;
  z-index: 10;
  background-color: black;
}
.leftContentScroll {
  top: 300px;
  width: 100%;
