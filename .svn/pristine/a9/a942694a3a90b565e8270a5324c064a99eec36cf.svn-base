  /* border-right: 0.5px solid grey; */

  /* border-right: 1px solid grey; */
}
.leftHeaderImage {
  height: 90%;
  width: 90%;
}
.leftHeaderImageContainer {
  flex: 1;
  padding: 0;
  /* border-right: 1px solid white; */
}
.leftHeaderHeading {
  /* flex: 3; */
