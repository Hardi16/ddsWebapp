body {
  background-color: black;
  /* #f3f3f3 */
}
.mainContainer {
  display: flex;
  flex-direction: row;
  background-color: black;
  width: 100%;
  color: white;
  min-height: 100vh;
  max-height: 100%;
}
.leftPane {
  flex: 1;
