  justify-content: space-between;
}
.leftPaneContent {
  display: flex;
  justify-content: center;
  padding: 30px 20px;
  /* border-right: 0.5px solid grey; */
}
form {
  width: 100%;
}

.addNewBtn {
  width: 100%;
  border-radius: 5px;
  padding: 20px;
