}
/* scroll*/
::-webkit-scrollbar {
  width: 10px;
}

::-webkit-scrollbar-track {
  background: rgb(36, 35, 35);
}

::-webkit-scrollbar-thumb {
  background: #555;
  opacity: 0.5;
}

