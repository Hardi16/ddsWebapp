  border-right: 1px solid gray;
  min-height: 100vh;
  width: 100px;
}
.leftPaneCreatePage {
  flex: 1;
  border-right: 1px solid gray;
  align-items: center;
  display: flex;
  flex-direction: column;
  overflow: scroll;
  max-height: 100%;
  /* background-color: rgb(56, 56, 56); */
}
.rightPane {
  flex: 3;
  /* border-left: 0.5px solid gray; */
}
.rightPaneCreatePage {
  flex: 3;
  overflow: scroll;
  max-height: 100%;
  height: 100vh;
}
.leftPaneHeader {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding-top: 20px;
  width: 100%;
  /* border-right: 0.5px solid grey; */

  /* border-right: 1px solid grey; */
}
.leftPaneHeaderDSCreate {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  /* padding-top: 10px; */
